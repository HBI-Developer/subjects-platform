rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /subjects/{subjectId} {
      allow read: if true;
      allow write: if request.auth != null;
    }

    match /resources/{resourceId} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    
    match /{document=**} {
      allow read, write: if false; 
    }
  }
}